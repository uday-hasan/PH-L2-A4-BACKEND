model Category {
  id          String     @id @default(uuid())
  name        String     @unique
  description String?
  status      STATUS     @default(ACTIVE)
  createdAt   DateTime   @default(now()) @db.Timestamptz(6)
  updatedAt   DateTime   @default(now()) @updatedAt @db.Timestamptz(6)
  medicine    Medicine[]
}
